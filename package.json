{
  "name": "ani-code",
  "version": "1.0.0",
  "license": "MIT",
  "bin": {
    "ani": "dist/core/cli.js"
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "ts-node --esm src/core/cli.ts",
    "start:win": "set NODE_OPTIONS=--loader ts-node/esm && node src/core/cli.ts",
    "build": "tsc",
    "dev": "set NODE_OPTIONS=--loader ts-node/esm && nodemon src/core/cli.ts",
    "test": "prettier --check . && xo && ava"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@babel/types": "^7.28.2",
    "@langchain/core": "^0.3.72",
    "@langchain/openai": "^0.6.9",
    "@types/fs-extra": "^11.0.4",
    "@types/node-fetch": "^2.6.13",
    "ajv": "^8.17.1",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "fs-extra": "^11.3.1",
    "ink": "^6.0.1",
    "inquirer": "^12.9.4",
    "meow": "^11.0.0",
    "neo4j-driver": "^5.28.1",
    "node-fetch": "^3.3.2",
    "open": "^10.2.0",
    "openai": "^5.16.0",
    "ora": "^8.2.0",
    "react": "^19.1.0",
    "socket.io": "^4.8.1",
    "tiktoken": "^1.0.22",
    "uri-js": "^4.4.1"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^3.0.1",
    "@swc/core": "^1.13.3",
    "@swc/helpers": "^0.5.17",
    "@types/babel__traverse": "^7.28.0",
    "@types/express": "^5.0.3",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.2.1",
    "@types/ora": "^3.1.0",
    "@types/react": "^19.1.8",
    "@vdemedes/prettier-config": "^2.0.1",
    "apidog-mcp-server": "^0.0.16",
    "ava": "^5.2.0",
    "chalk": "^5.2.0",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ink-testing-library": "^3.0.0",
    "prettier": "^2.8.7",
    "regenerator-runtime": "^0.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "xo": "^0.58.0"
  },
  "ava": {
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "xo": {
    "extends": "xo-react",
    "prettier": true,
    "rules": {
      "react/prop-types": "off"
    }
  },
  "prettier": "@vdemedes/prettier-config"
}
